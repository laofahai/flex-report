{
  "name": "@linch-kit/ai",
  "version": "1.0.0",
  "description": "LinchKit AI 集成层 - Graph RAG 知识图谱和 AI 工具",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "linch-kit-ai": "./dist/cli/main.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./extractors": {
      "types": "./dist/extractors/index.d.ts",
      "import": "./dist/extractors/index.js"
    },
    "./graph": {
      "types": "./dist/graph/index.d.ts",
      "import": "./dist/graph/index.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "typecheck": "tsc --noEmit",
    "extract": "bun run dist/cli/index.js"
  },
  "dependencies": {
    "@linch-kit/core": "workspace:*",
    "@linch-kit/schema": "workspace:*",
    "dotenv": "^16.4.7",
    "neo4j-driver": "^5.28.1",
    "neogma": "^1.12.5",
    "ts-morph": "^26.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "tsup": "^8.3.5",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "linch-kit",
    "ai",
    "graph-rag",
    "knowledge-graph",
    "neo4j",
    "data-extraction",
    "typescript"
  ],
  "author": "LinchKit Team",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}