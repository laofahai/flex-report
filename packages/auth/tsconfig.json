{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "react",
    "allowJs": true,
    "outDir": "dist",
    "noImplicitAny": true,
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ES2022",
    "strict": true
  },
  "include": ["src", "react"]
}