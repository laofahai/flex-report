{
  "name": "@linch-kit/types",
  "version": "1.0.0",
  "description": "TypeScript type definitions for Linch Kit",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf dist .turbo node_modules/.cache"
  },
  "keywords": ["typescript", "types", "linch-kit"],
  "author": "Linch Kit Team",
  "license": "MIT",
  "devDependencies": {
    "typescript": "5.8.3",
    "tsup": "^8.5.0"
  }
}
