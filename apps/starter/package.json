{
  "name": "linchkit-starter",
  "version": "1.0.0",
  "private": true,
  "description": "LinchKit Production Starter - AI-First 全栈开发框架",
  "keywords": [
    "linchkit",
    "starter",
    "production",
    "ai-first",
    "schema-driven",
    "full-stack"
  ],
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:quiet": "next dev 2>/dev/null",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "validate": "pnpm lint && pnpm type-check && pnpm build",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "schema:generate": "linchkit generate",
    "schema:validate": "linchkit validate",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@linch-kit/auth": "workspace:*",
    "@linch-kit/console": "workspace:*",
    "@linch-kit/core": "workspace:*",
    "@linch-kit/crud": "workspace:*",
    "@linch-kit/schema": "workspace:*",
    "@linch-kit/trpc": "workspace:*",
    "@linch-kit/ui": "workspace:*",
    "@prisma/client": "6.10.1",
    "@tanstack/react-query": "^5.62.10",
    "@trpc/client": "^11.0.0",
    "@trpc/next": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "date-fns": "^4.1.0",
    "next": "^15.3.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "superjson": "^2.2.1",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.3.4",
    "graceful-fs": "^4.2.11",
    "postcss": "^8.4.47",
    "prisma": "^6.10.1",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "^5.8.0",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "^18.3.0",
      "@types/react-dom": "^18.3.0"
    }
  }
}